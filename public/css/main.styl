@import "bootstrap.min.css"

@font-face
   font-family AndrewScript
   font-style normal
   src url(fonts/AndrewScript.ttf)

#logo
  font-family AndrewScript
  font-style bold
  font-size 500%
  line-height normal
  .a
    color #433B2E
    text-shadow 0 0 0.5em #F2D2C6, 0 0 0.5em #F2D2C6, 0 0 0.5em #F2D2C6, 0 0 0.5em #F2D2C6, 0 0 0.5em #F2D2C6, 0 0 0.5em #F2D2C6, 0 0 0.5em #F2D2C6, 0 0 0.5em #F2D2C6

body
  background: url('../img/background4.jpg') no-repeat center center fixed;
  -webkit-background-size: cover;
  -moz-background-size: cover;
  -o-background-size: cover;
  background-size: cover;

#actionResult
  display none

.providers
  margin: 5px 0;
  list-style: none;

  li
    display: inline;

.help-block {
  margin: -10px 0 10px 0;
  font-size: 10px;
  color: #ccc !important;
}
h1 {
  margin-bottom: 20px !important;
}
.loggedin, .loggedout
  display none